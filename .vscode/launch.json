    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch All Workers",
            "program": "${workspaceFolder}/bin/cvmfe.js",
            "args": [
                "run",
                "all"
            ],
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch CVMDataProcess Worker",
            "program": "${workspaceFolder}/bin/cvmfe.js",
            "args": [
                "run",
                "cvmDataProcess"
            ],
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch CVMStatisticProcess Worker",
            "program": "${workspaceFolder}/bin/cvmfe.js",
            "args": [
                "run",
                "cvmStatisticProcess"
            ],
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch XPIFundProcess Worker",
            "program": "${workspaceFolder}/bin/cvmfe.js",
            "args": [
                "run",
                "xpiFundProcess"
            ],
            "console": "integratedTerminal"
        }
    ]
}